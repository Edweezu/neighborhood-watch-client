{
    "version": 2,
    "alias": "neighborhood-watch-client",
    "name": "neighborhood-watch-client",
    "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json",
        "headers": {"Content-Type": "application/json"}
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}